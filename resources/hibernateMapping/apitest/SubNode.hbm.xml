<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.apitet.res.entity.SubNode" table="resource_type">
		<id name="id" column="ID" type="java.lang.String">
			<generator class="assigned" />
		</id>
		
		
		<property name="ownerAssembly" column="owner_assembly" type="java.lang.String" />
		<property name="text" column="NAME" type="java.lang.String" />
		<property name="drmId" column="drm_id" type="java.lang.String" />
		
	</class>
</hibernate-mapping>
