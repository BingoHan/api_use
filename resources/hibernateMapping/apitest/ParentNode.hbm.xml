<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.apitet.res.entity.ParentNode" table="dynamic_res_module">
		<id name="id" column="ID" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="caption" column="caption" type="java.lang.String" />
		<property name="text" column="name" type="java.lang.String" />
		<property name="projId" column="proj_id" type="java.lang.String" />
	</class>
</hibernate-mapping>
